    "trusted_uri_prefixes":[
       "https://<ICP_ENDPOINT_URL>:<ICP_ENDPOINT_PORT>", "https://<ICP_PROXY_IP>"    ],
    "redirect_uris":["https://<ICP_PROXY_IP>:<ICP_PROXY_PORT>/auth/liberty/callback"]
